{
  "name": "generator-exprest-api",
  "version": "3.4.1",
  "description": "REST API generator with Express",
  "repository": "arthurfauq/generator-exprest-api",
  "homepage": "https://github.com/arthurfauq/generator-exprest-api",
  "author": {
    "name": "Arthur Fauquenot",
    "email": "arthurfauquenot@yahoo.com",
    "url": "https://github.com/arthurfauq"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator",
    "generator",
    "generators",
    "yeoman",
    "nodejs",
    "rest",
    "rest-api",
    "restful",
    "node",
    "nodejs",
    "express",
    "winston",
    "swagger",
    "docker"
  ],
  "scripts": {
    "lint": "eslint . && prettier --check \"**/*.{json,md,html,yaml}\"",
    "lint:fix": "eslint . --fix && prettier --write \"**/*.{json,md,html,yaml}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,html,yaml}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "validate-npm-package-name": "^3.0.0",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "devDependencies": {
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2"
  },
  "engines": {
    "npm": ">= 10.0.0"
  },
  "license": "MIT"
}
